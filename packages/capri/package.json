{
  "name": "capri",
  "version": "6.0.0-next.1",
  "description": "Static site generator for Vite",
  "author": "Felix Gnass <fgnass@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/capri-js/capri"
  },
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "ssr.d.ts"
  ],
  "exports": {
    ".": {
      "default": "./lib/index.js"
    },
    "./context": {
      "default": "./lib/context.js"
    },
    "./vite-plugin": {
      "default": "./lib/vite-plugin.js"
    }
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "lib/index.d.ts"
      ],
      "context": [
        "lib/context.d.ts"
      ],
      "vite-plugin": [
        "lib/vite-plugin.d.ts"
      ],
      "ssr": [
        "ssr.d.ts"
      ]
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "micromatch": "^4.0.5",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.6",
    "rollup": "^4.18.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  }
}
